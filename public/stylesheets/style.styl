body
  margin: 0px
  padding: 0px
  background-color: pink
  font: 16px Helvetica, Georgia, serif
a
  color: #00B7FF
#page-wrap
  margin: 2% auto
  color: white
#page-wrap > .container
  border: 1px solid #999
  padding: 1%
  margin-bottom: 1px
  color: black

section > div
  padding: 20px

header, nav, section, footer
  background-color: #eee
  text-align: center

nav span
  margin: 0 10px 0 0

header
  div
    margin: 0 auto;
    text-align: center
    width: 75%
    height: 15%
    border: 3px solid #73AD21
  img
    float: left
    width: 8%
    height: 8%
#portada
  text-align: center
  margin:10px

#container
  clear: left
  width: 100%
#contenedor
  border: 3px solid #73AD21
  width: 60%
  margin: auto
  font-size: 200%;
.Marcador
  border: 3px solid #73AD21
.Pregunta
  border: 3px solid #73AD21

